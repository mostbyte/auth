{
  "name": "mostbyte/auth",
  "description": "Mostbyte authorization system from identity service",
  "type": "library",
  "license": "MIT",
  "require": {
    "php": "^8.2",
    "illuminate/support": "^11.0|^12.0",
    "illuminate/contracts": "^11.0|^12.0",
    "illuminate/database": "^11.0|^12.0",
    "guzzlehttp/guzzle": "^7.8"
  },
  "require-dev": {
